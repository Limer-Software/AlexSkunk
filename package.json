{
	"name": "alexskunk",
	"version": "0.1.0-dev",
	"description": "A Discord bot.",
	"main": "dist/index.js",
	"types": "src/types/index.d.ts",
	"scripts": {
		"start": "pm2 start pm2.config.js --env production",
		"debug": "nodemon .",
		"build": "tsc",
		"build:dev": "tsc -w",
		"setup": "npm i && npm run build",
		"migrate": "node-pg-migrate"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RobotoSkunk/AlexSkunk.git"
	},
	"keywords": [
		"alex",
		"skunk",
		"bot",
		"discord",
		"alexskunk",
		"cute"
	],
	"author": "RobotoSkunk (contact@robotoskunk.com)",
	"license": "GNU AGPLv3",
	"bugs": {
		"url": "https://github.com/RobotoSkunk/AlexSkunk/issues"
	},
	"homepage": "https://github.com/RobotoSkunk/AlexSkunk#readme",
	"nodemonConfig": {
		"watch": [
			"dist/"
		],
		"delay": "250",
		"env": {
			"NODE_ENV": "development"
		}
	},
	"dependencies": {
		"@discordjs/rest": "^1.7.1",
		"discord-api-types": "^0.37.47",
		"discord.js": "^14.11.0",
		"dotenv": "^16.3.1",
		"morgan": "^1.10.0",
		"ms": "^2.1.3",
		"node-pg-migrate": "^6.2.2",
		"pg": "^8.11.1",
		"rate-limiter-flexible": "^2.4.1",
		"source-map-support": "^0.5.21",
		"winston": "^3.9.0"
	},
	"devDependencies": {
		"@types/pg": "^8.10.2"
	}
}
